        rockchip,camera-module-lens-name = "30IRC-4M-F20";
        port {
            gc08a8_dphy1_out: endpoint {
                remote-endpoint = <&dphy1_in_gc08a8>;
                data-lanes = <1 2>;
            };
        };
    };

    dphy1_gc2053: gc2053@37 {
        compatible = "galaxycore,gc2053";
        status = "disabled";
        reg = <0x37>;
        clocks = <&ext_cam_24m_clk>;
        clock-names = "xvclk";
        dovdd-supply = <&cam_dovdd>;
